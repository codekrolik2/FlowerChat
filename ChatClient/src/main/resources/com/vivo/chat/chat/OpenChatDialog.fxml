<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root prefHeight="78.0" prefWidth="280.0" type="VBox" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1">
   <HBox alignment="CENTER" maxHeight="40.0" minHeight="40.0" prefHeight="40.0">
      <children>
            <Label text="With users (comma-separated) " />
      </children>
   </HBox>
   <HBox alignment="CENTER" maxHeight="40.0" minHeight="40.0" prefHeight="40.0">
      <children>
            <TextField fx:id="usernamesTextField" onKeyReleased="#formKeyRelease" prefHeight="24.0" prefWidth="259.0" />
      </children>
   </HBox>
    <HBox alignment="CENTER" maxHeight="40.0" minHeight="40.0" prefHeight="40.0">
      <children>
           <Button fx:id="startChatButton" alignment="CENTER" mnemonicParsing="false" onAction="#startChatAction" text="Connect" />
      </children>
    </HBox>
</fx:root>
